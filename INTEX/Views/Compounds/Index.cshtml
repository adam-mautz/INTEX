@model IEnumerable<INTEX.Models.Compounds>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Compound_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Compund_Quantity)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Appearance)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Estimated_Weight)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Date_Arrived)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Date_Received)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Date_Due)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Actual_Weight)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Max_Tolerated_Dose)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Compound_Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Compund_Quantity)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Appearance)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Estimated_Weight)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Date_Arrived)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Date_Received)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Date_Due)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Actual_Weight)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Max_Tolerated_Dose)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.LT_Number }) |
            @Html.ActionLink("Details", "Details", new { id=item.LT_Number }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.LT_Number })
        </td>
    </tr>
}

</table>
